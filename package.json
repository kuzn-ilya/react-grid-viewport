    "@types/chai-spies": "0.0.0",
    "@types/mocha": "^2.2.32",
    "@types/object-assign": "^4.0.30",
    "@types/react": "^0.14.44",
    "@types/react-addons-test-utils": "^0.14.15",
    "@types/react-dom": "^0.14.18",
