{
  "name": "react-scrollable-container",
  "version": "0.0.1",
  "description": "React Scrollable Container",
  "main": "app.js",
  "scripts": {
    "example-grid": "cross-env NODE_ENV=development webpack-dev-server --config ./examples/grid/webpack.config.js",
    "test": "mocha --require ignore-styles --compilers ts:ts-node/register,tsx:ts-node/register ./tests/**/*.spec.tsx",
    "tslint": "tslint sources/**/*.ts{,x}",
    "test:coverage": "nyc npm run test",
    "test:karma": "karma start"
  },
  "nyc": {
    "include": [
      "sources/*.ts",
      "sources/**/*.ts",
      "sources/*.tsx",
      "sources/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuzn-ilya/react-scrollable-container.git"
  },
  "author": "Ilya Kuznetsov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kuzn-ilya/react-scrollable-container.git/issues"
  },
  "homepage": "https://github.com/kuzn-ilya/react-scrollable-container.git#readme",
  "dependencies": {
    "object-assign": "^4.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/chai-enzyme": "^0.5.4",
    "@types/enzyme": "^2.5.37",
    "@types/jsdom": "^2.0.29",
    "@types/mocha": "^2.2.32",
    "@types/object-assign": "^4.0.30",
    "@types/react": "^0.14.44",
    "@types/react-addons-test-utils": "^0.14.15",
    "@types/react-dom": "^0.14.18",
    "awesome-typescript-loader": "^2.2.4",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "ejs-loader": "^0.3.0",
    "enzyme": "^2.6.0",
    "html-webpack-plugin": "^2.24.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.8.3",
    "jsdom-global": "^2.1.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.0",
