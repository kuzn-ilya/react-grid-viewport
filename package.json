{
  "name": "react-scrollable-container",
  "version": "0.0.1",
  "description": "React Scrollable Container",
  "scripts": {
    "build": "npm run tslint && npm run test && npm run build:prod",
    "build:prod": "webpack --config ./config/webpack.prod.js",
    "clean": "rimraf ./node_modules ./build ./coverage",
    "test:coverage": "npm run test:coverage:remap && npm run test:coverage:report",
    "test:coverage:remap": "remap-istanbul -i coverage/coverage.json -o coverage/coverage.json -t json -e node_modules,test,app.ts",
    "test:coverage:report": "istanbul report",
    "test:karma": "karma start",
    "test:chrome": "karma start --browsers Chrome",
    "test:ff": "karma start --browsers Firefox",
    "test:ie": "karma start --browsers IE",
    "test:all": "karma start --browsers PhantomJSCustom,Chrome,Firefox,IE",
    "examples": "webpack-dev-server --config ./config/webpack.dev.js",
    "test": "npm run test:karma && npm run test:coverage",
    "tslint": "tslint sources/**/*.ts{,x}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuzn-ilya/react-scrollable-container.git"
  },
  "author": "Ilya Kuznetsov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kuzn-ilya/react-scrollable-container.git/issues"
  },
  "homepage": "https://github.com/kuzn-ilya/react-scrollable-container.git#readme",
  "dependencies": {
    "fbjs": "^0.8.11",
    "immutable": "^3.8.1",
    "object-assign": "^4.1.1",
    "prop-types": "15.5.8",
    "react": "15.5.4",
    "react-dom": "15.5.4"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/chai-spies": "0.0.0",
    "@types/mocha": "^2.2.41",
    "@types/node": "^6.0.72",
    "@types/object-assign": "^4.0.30",
    "@types/prop-types": "15.5.1",
    "@types/react": "0.14.57",
    "@types/react-addons-perf": "0.14.18",
    "@types/react-addons-test-utils": "0.14.17",
    "@types/react-dom": "0.14.23",
    "@types/ua-parser-js": "^0.7.30",
    "autoprefixer": "^6.7.7",
    "awesome-typescript-loader": "^3.1.3",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "css-loader": "^0.25.0",
    "ejs-loader": "^0.3.0",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.1",
    "istanbul-instrumenter-loader": "0.2.0",
    "json-loader": "^0.5.4",
    "karma": "^1.6.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.1.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-coverage": "^0.1.4",
    "karma-source-map-support": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
