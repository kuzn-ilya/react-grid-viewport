  "description": "React Scrollable Container",
  "scripts": {
    "build": "npm run tslint && npm run test && npm run build:prod",
    "build:prod": "webpack --config ./config/webpack.prod.js",
    "clean": "rimraf ./node_modules ./build ./coverage",
    "coverage": "npm run coverage:remap && npm run coverage:report",
    "coverage:remap": "remap-istanbul -i coverage/coverage.json -o coverage/coverage.json -t json -e node_modules,test,app.ts",
    "coverage:report": "istanbul report",
    "examples": "webpack-dev-server --config ./config/webpack.dev.js",
    "test": "karma start",
    "posttest": "npm run coverage",
    "tslint": "tslint sources/**/*.ts{,x}"
  },
