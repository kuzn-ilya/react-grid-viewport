  },
  "homepage": "https://github.com/kuzn-ilya/react-scrollable-container.git#readme",
  "dependencies": {
    "object-assign": "^4.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
